{
  "name": "sellify-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "services/*",
    "packages/*",
    "examples/*",
    "tests/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,css,mjs,json}\"",
